, setFormData] = useState({\n    name: \"\",\n    time: \"\",\n    img: \"\"\n  })\n  const { id } = useParams()\n\n  useEffect(() => {\n    const fillForm = () => {\n      const recipeItem = props.recipes.find(recipe => recipe.id === Number(id))\n      setFormData({\n        name: recipeItem.name,\n        time: recipeItem.time,\n        img: recipeItem.img\n      })\n    }\n    if (props.recipes.length) {\n      fillForm()\n    }\n  }, [props.recipes])\n  \n\n  const handleChange = (e) => {\n    const { name, value } = e.target\n    setFormData(prevState => ({\n      ...prevState,\n      [name]: value\n    }))\n  }\n\n  return (\n    <div className=\"edit-ultra-container\">\n      <div className='edit-page-container'>\n    <form className=\"edit-form\"\n      onSubmit={(e) => {\n      e.preventDefault()\n      props.handleUpdate(id, formData)\n    }}>\n      <h3>Edit Your Recipe</h3>\n      <label>Recipe Name:\n    <input\n          type='text'\n          name='name'\n          value={formData.name}\n          onChange={handleChange}\n        />\n      </label>\n      <label>Cook Time:\n    <input\n          type='text'\n          name='time'\n          value={formData.time}\n          onChange={handleChange}\n        />\n      </label>\n      <label>Recipe Image:\n    <input\n          type='text'\n          name='img'\n          value={formData.img}\n          onChange={handleChange}\n        />\n      </label>\n      <button>Submit</button>\n      </form>\n    </div>\n    </div>\n  )\n}","/Users/davidmurphy/Desktop/GeneralAssebly/phoenix/FinalProject/Cooked/client/src/screens/AddIngredients.jsx",[],{"ruleId":"101","replacedBy":"102"},{"ruleId":"103","replacedBy":"104"},{"ruleId":"105","severity":1,"message":"106","line":22,"column":6,"nodeType":"107","endLine":22,"endColumn":8,"suggestions":"108"},{"ruleId":"101","replacedBy":"109"},{"ruleId":"103","replacedBy":"110"},{"ruleId":"111","severity":1,"message":"112","line":15,"column":15,"nodeType":"113","endLine":