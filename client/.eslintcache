>\n        <Route path='/'>\n          <Login\n            handleLogin={handleLogin}\n            handleRegister={handleRegister}\n          />\n        </Route>\n        \n        </Switch>\n      </Layout>\n  );\n}\nexport default App;","/Users/davidmurphy/Desktop/GeneralAssebly/phoenix/FinalProject/Cooked/client/src/reportWebVitals.js",[],"/Users/davidmurphy/Desktop/GeneralAssebly/phoenix/FinalProject/Cooked/client/src/layouts/layout.jsx",[],"/Users/davidmurphy/Desktop/GeneralAssebly/phoenix/FinalProject/Cooked/client/src/components/Header.jsx",[],"/Users/davidmurphy/Desktop/GeneralAssebly/phoenix/FinalProject/Cooked/client/src/screens/Login.jsx",[],"/Users/davidmurphy/Desktop/GeneralAssebly/phoenix/FinalProject/Cooked/client/src/services/auth.js",[],"/Users/davidmurphy/Desktop/GeneralAssebly/phoenix/FinalProject/Cooked/client/src/services/apiConfig.js",[],"/Users/davidmurphy/Desktop/GeneralAssebly/phoenix/FinalProject/Cooked/client/src/screens/Register.jsx",[],"/Users/davidmurphy/Desktop/GeneralAssebly/phoenix/FinalProject/Cooked/client/src/screens/Home.jsx",[],["91","92"],"/Users/davidmurphy/Desktop/GeneralAssebly/phoenix/FinalProject/Cooked/client/src/components/Recipes.jsx",["93"],"import { Link } from 'react-router-dom'\nimport './Recipes.css'\n\n\nexport default function Recipes(props) {\n  \n  return (\n    <div className=\"recipe-ultra-container\">\n    <div className=\"recipe-page-container\">\n      {props.recipes && props.recipes.map(recipe =>\n        <Link to={`/recipes/${recipe.id}`} key={recipe.id}>\n          <div className=\"recipe-container\">\n            <h4 className=\"recipe-name recipe\">{recipe.name}</h4>\n            <h5 className=\"recipe-name recipe\">Cook Time: {recipe.time}</h5>\n              <img className=\"recipe-image\" src={recipe.img} />\n          </div>\n        </Link>\n      )}\n        </div>\n      </div>\n  )\n}","/Users/davidmurphy/Desktop/GeneralAssebly/phoenix/FinalProject/Cooked/client/src/services/recipes.js",[],"/U