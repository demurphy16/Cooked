await getOneRecipe(Number(id))\n      setRec(ingredientsData)\n      console.log(rec)\n    }\n    fetchIngredients()\n  }, [])\n\n  return (\n    <div className='more-details-ultra-container'>\n      <div className=\"more-details-page-container\">\n        <h2 className=\"more-details-title\">{rec.name}</h2>\n        <div>\n        <img className=\"more-details-img\"src={rec.img} />\n        </div>\n          <div className=\"more-details-container\">\n          {rec.ingredients && rec.ingredients.map(ingredient =>\n            <div className=\"ingredients-container\">\n              <h4 className=\"more-details-name ing\">{`${ingredient.name}, ${ingredient.quantity}`}</h4>\n            </div>)}\n        </div>\n        \n        <button className=\"add-ingredients-button\"\n          onClick={\n          () => { ingredientToggle ? setIngredientToggle(false) : setIngredientToggle(true) }}>\n          Add Ingredients</button>\n          {ingredientToggle &&\n          <AddIngredients handleAddIngredient={props.handleAddIngredient}/>\n        } \n        {rec.user_id && rec.user_id === props.currentUser?.id ?\n        < div className=\"more-details-button-container\">\n        <button className=\"details-button\" onClick={() => props.handleDelete(rec.id)}>Delete Recipe</button>\n        <Link to={`/recipes/${rec.id}/edit`}><button className=\"details-button\">Edit Recipe</button></Link>\n      </div>\n      : <div></div>} \n      </div>\n      \n      </div>\n  )\n}","/Users/davidmurphy/Desktop/GeneralAssebly/phoenix/FinalProject/Cooked/client/src/services/ingredients.js",[],"/Users/davidmurphy/Desktop/GeneralAssebly/phoenix/FinalProject/Cooked/client/src/screens/RecipeCreate.jsx",[],"/Users/davidmurphy/Desktop/GeneralAssebly/phoenix/FinalProject/Cooked/client/src/containers/MainContainer.jsx",["99","100"],"/Users/davidmurphy/Desktop/GeneralAssebly/phoenix/FinalProject/Cooked/client/src/screens/RecipeEdit.jsx",["101"],"import {useState, useEffect} from 'react'\nimport { useParams } from 'react-router-dom'\n\nimport './RecipeEdit.css'\n\nexport default function RecipeEdit(props) {\n\n  const [formData, setFormData] = useState({\n    name: \"\",\n    time: \"\",\n    img: \"\"\n  })\n  const { id } = useParams()\n\n  useEffect(() => {\n    const fillForm = () => {\n      const recipeItem = props.recipes.find(recipe => recipe.id === Number(id))\n      setFormData({\n        name: recipeItem.name,\n        time: recipeItem.time,\n        img: recipeItem.img\n      })\n    }\n    if (props.recipes.length) {\n      fillForm()\n    }\n  }, [props.recipes])\n  \n\n  const handleChange = (e) => {\n    const { name, value } = e.target\n    setFormData(prevState => ({\n      ...prevState,\n      [name]: value\n    }))\n  }\n\n  return (\n    <div className=\"edit-ultra-container\">\n      <div className='edit-page-container'>\n    <form className=\"edit-form\"\n      onSubmit={(e) => {\n      e.preventDefault()\n      props.handleUpdate(id, formData)\n    }}>\n      <h3>Edit Your Recipe</h3>\n      <label>Recipe Name:\n    <input\n          type='text'\n          name='name'\n          value={formData.name}\n          onChange={handleChange}\n        />\n      </label>\n      <label>Cook Time:\n    <input\n          type='text'\n          name='time'\n          value={formData.time}\n          onChange={handleChange}\n        />\n      </label>\n      <label>Recipe Image:\n    <input\n          type='text'\n          name='img'\n          value={formData.img}\n          onChange={handleChange}\n        />\n      </label>\n      <button>Submit</button>\n      </form>\n    </div>\n    </div>\n  )\n}","/Users/davidmurphy/Desktop/GeneralAssebly/phoenix/FinalProject/Cooked/client/src/screens/AddIngredients.jsx",[],{"ruleId":"102","replacedBy":"103"},{"ruleId":"104","replacedBy":"105"},{"ruleId":"106","severity":1,"message":"107","line":22,"column":6,"nodeType":"108","endLine":22,"endColumn":8,"suggestions":"109"},{"ruleId":"102","replacedBy":"1